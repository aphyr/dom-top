#kaocha/v1
{:capture-output? true,
 :color?          true,
 :fail-fast?      true,
 :plugins         [:kaocha.plugin/print-invocations
                   :noyoda.plugin/swap-actual-and-expected],
 :reporter        kaocha.report/documentation,
 :tests           [{:id           :unit,
                    :ns-patterns  ["dom-top.*-test$"],
                    :source-paths ["src"],
                    :test-paths   ["test"]
                    :kaocha.filter/skip-meta [:perf]}],
 :watch?          false}
